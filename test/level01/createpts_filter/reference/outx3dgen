# For a unit cell divided into PTS**3 sub-cells,                                
# delta-x = 0.01 = distance between sub-cell centers                            
# x_p = delta-x / 100 = 0.0001 = 1% perturbations                               
# eps_1 = sqrt(3*x_p) = 0.000173 = max dist. from origin                        
define/XP/0.0001                                                                
define/EPS1/0.00018                                                             
define/PTS/101                                                                  
